<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/theme/**</from>
		<to>/theme/$1</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
	<!-- FIX: El path es gobiernodecanarias.org/istac/buscador-istac-web -->
	<!-- Según la configuración de tomcat-apache quitar este FIX	-->
<!--	<outbound-rule>-->
<!--		<from>/**</from>-->
<!--		<to>/istac/$1</to>-->
<!--	</outbound-rule>-->

</urlrewrite>
